{
  "name": "vite-tauri-template",
  "description": "Vite + Tauri + Vuetify bundled together to use",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "serve": "vite",
    "serve:tauri": "VITE_NETWORK_PROCESSOR=tauri vite",
    "serve:native": "tauri dev",
    "build": "VITE_NETWORK_PROCESSOR=tauri vite build && tauri build",
    "preview": "vite preview",
    "ts:check": "vue-tsc --noEmit",
    "gen:proto": "pbjs --es6 -t static-module -w es6 -o src/generated/proto.js $(ls ../roboteam_networking/proto/* | grep -xv \"../roboteam_networking/proto/CMakeLists.txt\") &&  pbts --pb3-optional -o src/generated/proto.d.ts src/generated/proto.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@mdi/font": "7.1.96",
    "@tauri-apps/api": "1.2.0",
    "@vueuse/core": "^10.0.2",
    "daisyui": "^2.51.5",
    "mitt": "^3.0.0",
    "pinia": "^2.0.34",
    "pinia-plugin-persistedstate": "^3.1.0",
    "pixi.js": "^7.2.4",
    "protobufjs": "^7.2.3",
    "roboto-fontface": "0.10.0",
    "unified-network": "0.6.4",
    "vue": "3.2.47"
  },
  "devDependencies": {
    "@savvywombat/tailwindcss-grid-areas": "^3.0.1",
    "@tauri-apps/cli": "1.2.3",
    "@vitejs/plugin-vue": "4.0.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "protobufjs-cli": "^1.1.1",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.4",
    "vite": "4.1.1",
    "vue-tsc": "^1.6.1"
  },
  "main": "index.ts"
}
